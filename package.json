{
  "name": "s4qs-rs",
  "version": "3.2.0",
  "description": "Reads S3 object creation events from SQS and copies data to Redshift",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "test-cov": "mocha --require blanket -R html-cov > coverage.html"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ORBAT/S4QS-RS.git"
  },
  "bin": "./app.js",
  "keywords": [
    "s3",
    "sqs",
    "redshift"
  ],
  "dependencies": {
    "aws-sdk": "2.x",
    "bluebird": "2.x",
    "co": "^4.6.0",
    "config": "1.x",
    "debug": "2.x",
    "express": "4.x",
    "highland": "caolan/highland#0cd59e9a810ab9472f5f00dd752e09980de5989d",
    "lodash": "3.x",
    "lru-cache": "4.0.1",
    "node-etcd": "^4.2.1",
    "pg": "4.x",
    "promise-pauser": "^1.0.0",
    "statsd-client": "0.2.0",
    "zabbix-sender": "0.2.0"
  },
  "devDependencies": {
    "mocha": "2.x",
    "chai": "1.x",
    "sinon": "1.12",
    "blanket": "1.x",
    "sinon-chai": "2.x",
    "mocha-sinon": "1.x",
    "chai-as-promised": "4.x",
    "sinon-as-promised": "2.x"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "config": {
    "blanket": {
      "pattern": "lib",
      "data-cover-never": "node_modules"
    }
  },
  "license": "MIT",
  "author": "Tom Ekl√∂f <tome@unity3d.com>",
  "bugs": {
    "url": "https://github.com/ORBAT/S4QS-RS/issues"
  },
  "homepage": "https://github.com/ORBAT/S4QS-RS"
}

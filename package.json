{
  "name": "s4qs-rs",
  "version": "0.4.0",
  "description": "Reads S3 object creation events from SQS and copies data to Redshift",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "test-cov": "mocha --require blanket -R html-cov > coverage.html"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ORBAT/S4QS-RS.git"
  },
  "bin": "./app.js",
  "keywords": [
    "s3",
    "sqs",
    "redshift"
  ],
  "dependencies": {
    "aws-sdk": "2.x",
    "lodash": "3.x",
    "bluebird": "2.x",
    "debug": "2.x",
    "lru-cache": "2.5.x",
    "pg": "4.x",
    "config": "1.x",
    "express": "4.x"
  },
  "devDependencies": {
    "mocha": "2.x",
    "chai": "1.x",
    "blanket": "1.x",
    "sinon-chai": "2.x",
    "mocha-sinon": "1.x",
    "chai-as-promised": "4.x"
  },
  "config": {
    "blanket": {
      "pattern": "lib",
      "data-cover-never": "node_modules"
    }
  },
  "license": "MIT",
  "author": "Tom Ekl√∂f <tome@unity3d.com>",
  "bugs": {
    "url": "https://github.com/ORBAT/S4QS-RS/issues"
  },
  "homepage": "https://github.com/ORBAT/S4QS-RS"
}
